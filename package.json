{
  "name": "rollup-plugin-unassert",
  "version": "0.6.0",
  "description": "RollupJS plugin to remove assertion calls via Unassert",
  "main": "dist/rollup-plugin-unassert.cjs.js",
  "module": "dist/rollup-plugin-unassert.esm.js",
  "types": "dist/rollup-plugin-unassert.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/rollup-plugin-unassert.cjs.d.ts",
        "require": "./dist/rollup-plugin-unassert.esm.d.ts"
      },
      "import": "./dist/rollup-plugin-unassert.cjs.js",
      "require": "./dist/rollup-plugin-unassert.esm.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "@rollup/pluginutils": "^4.2.1",
    "unassert": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^8.21.0",
    "eslint-config-mourner": "^3.0.0",
    "rollup": "^2.77.3",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.4.0",
    "tape": "^5.5.3"
  },
  "scripts": {
    "test": "node test/test.js",
    "pretest": "npm run lint && npm run build",
    "build": "rollup -c",
    "lint": "eslint src test/test.js rollup.config.js",
    "prepublishOnly": "npm test"
  },
  "eslintConfig": {
    "extends": "mourner",
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "no-process-exit": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mourner/rollup-plugin-unassert.git"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "unassert"
  ],
  "author": "Iván Sánchez Ortega <ivan@sanchezortega.es>",
  "license": "Beerware",
  "bugs": {
    "url": "https://github.com/mourner/rollup-plugin-unassert/issues"
  },
  "homepage": "https://github.com/mourner/rollup-plugin-unassert"
}
